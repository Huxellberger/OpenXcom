<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros">
    <COMMON_DAT_FOLDER>common</COMMON_DAT_FOLDER>
    <STANDARD_DAT_FOLDER>standard</STANDARD_DAT_FOLDER>
    <TFTD_DAT_FOLDER>TFTD</TFTD_DAT_FOLDER>
    <UFO_DAT_FOLDER>UFO</UFO_DAT_FOLDER>
  </PropertyGroup>
  <PropertyGroup />
  <ItemDefinitionGroup />
  <ItemGroup>
    <BuildMacro Include="COMMON_DAT_FOLDER">
      <Value>$(COMMON_DAT_FOLDER)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
    <BuildMacro Include="STANDARD_DAT_FOLDER">
      <Value>$(STANDARD_DAT_FOLDER)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
    <BuildMacro Include="TFTD_DAT_FOLDER">
      <Value>$(TFTD_DAT_FOLDER)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
    <BuildMacro Include="UFO_DAT_FOLDER">
      <Value>$(UFO_DAT_FOLDER)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
  </ItemGroup>
</Project>